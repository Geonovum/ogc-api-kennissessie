meta {
  name: items zoom-level
  type: http
  seq: 1
}

get {
  url: {{serviceHost}}/collections/{{dataset}}/items?f=html&zoom-level=3
  body: none
  auth: inherit
}

params:query {
  f: html
  zoom-level: 3
}

headers {
  Accept: application/geo+json
}

settings {
  encodeUrl: true
  timeout: 0
}
